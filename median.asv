function [outputImage] = median(inputImage,kernelSize)
    %channels of rgb image are separated
    red = inputImage(:, :, 1);
    green = inputImage(:, :, 2);
    blue = inputImage(:, :, 3);
    
    I_red=medfilt2(red,[3,3]);
    I_green=medfilt2(green,[3,3]);
    I_blue=medfilt2(blue,[3,3]);
    
    outputImage(:, :, 1)=I_red;
    outputImage(:, :, 2)=I_green;
    outputImage(:, :, 3)=I_blue;
end

